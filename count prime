class Solution {
public:
    int countPrimes(int n) {
        int count=0;
        vector<bool>prime(n+1,true);
        prime[0]=prime[1]=false;
        int i,j;
        for(i=2;i<n;i++)
        {
            if(prime[i])
            {
            count++;
            for(j=2*i;j<n;j=j+i)
              prime[j]=0;
            }
        }
        return count;
    }
};
